@model IEnumerable<SlujitelModel>
@Html.Partial("_IndexHead", Model)
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TekushtaData)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td><a asp-action="Edit" asp-route-id="@item.Id">@item.Number</a></td>
            <td asp-display-for="@item.Name"></td>
            <td asp-display-for="@item.TekushtaData"></td>
            <td asp-display-for="@item.User"></td>
        </tr>
}
    </tbody>
</table>

<partial name="_Pager" />